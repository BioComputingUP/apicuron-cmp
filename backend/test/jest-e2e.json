{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@biocomputingup/nestjs-kurrent": "<rootDir>/../libs/nestjs-kurrent/src",
    "@biocomputingup/nestjs-kurrent/(.*)": "<rootDir>/../libs/nestjs-kurrent/src/$1"
  }
}